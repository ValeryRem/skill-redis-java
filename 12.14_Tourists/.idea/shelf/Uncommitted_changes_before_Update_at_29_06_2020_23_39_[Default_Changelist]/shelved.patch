+import java.util.Set;
+import java.util.concurrent.ConcurrentHashMap;
+import java.util.concurrent.ExecutorService;
+import java.util.concurrent.Executors;
+import java.util.concurrent.TimeUnit;
+import java.util.concurrent.atomic.LongAdder;
+
+public class Main {
+    private static final int PAGES_COUNT = 100;
+    static final Set<Integer> alreadyParsed = ConcurrentHashMap.newKeySet();
+    static LongAdder counter = new LongAdder();
+    static ExecutorService ex = Executors.newFixedThreadPool(2);
+
+    public static void main(String[] args) throws InterruptedException {
+        for (int pageId = 0; pageId < PAGES_COUNT; pageId++) {
+            parsePage(pageId);
+            parsePage(pageId);
+        }
+
+        ex.shutdown();
+        ex.awaitTermination(1, TimeUnit.HOURS);
+
+        System.out.printf("done. Total parsed: %d/%d %n", counter.intValue(), PAGES_COUNT);
+    }
+
+    private static void parsePage(int pageId) {
+        ex.execute(() -> {
+            try {
+                Thread.sleep(1);
+            } catch (InterruptedException e) {
+                e.printStackTrace();
+            }
+           synchronized (alreadyParsed) {
+               if (!alreadyParsed.contains(pageId)) {
+                   alreadyParsed.add(pageId);
+                   System.out.println("Parse page" + pageId);
+                   counter.increment();
+               }
+           }
+        });
+    }
+}
